{
  "extends": ["tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"],
  "defaultSeverity": "warning",
  "member-access": [true, "nopublic"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "interface-name" : [true, "never-prefix"]
  }
}
